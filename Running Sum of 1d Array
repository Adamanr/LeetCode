public class Solution {
        public  int[] RunningSum(int[] nums)
        {
            int l = nums.Length;
            int[] aqua = new int[l];
            int score = 0;
            for (int i = 0; i < nums.Length; i++)
            {
                score += nums[i];
                aqua[i] = score;
            }
            return aqua;

        }
}
